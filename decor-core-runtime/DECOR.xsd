                            <xs:attribute name="expirationDate" use="optional"/>
                            <xs:attribute name="lastModifiedDate" use="optional"/>
                            <!-- from old times for backward compatibility -->
                            <xs:attribute name="statusCode" type="CodedConceptStatusCodeLifeCycleStable" use="optional" default="active">
