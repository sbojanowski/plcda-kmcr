            <xs:documentation xml:lang="en-US">Life cycle of the Status Code of a FHIR Profile</xs:documentation>
        </xs:annotation>
        <xs:restriction base="EnumerationToken">
            <xs:enumeration value="draft">
                <xs:annotation>
