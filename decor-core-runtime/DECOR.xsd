                </xs:annotation>
            </xs:element>
            <xs:element name="answerOption" minOccurs="0" maxOccurs="unbounded" type="AnswerOptionType">
                <xs:annotation>
                    <xs:documentation xml:lang="en-US">Valueset containing permitted answers.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="initial" minOccurs="0" maxOccurs="unbounded" type="InitialOptionType">
